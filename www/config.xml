<?xml version="1.0" encoding="UTF-8" ?>
<widget
	xmlns:gap="http://phonegap.com/ns/1.0"
	id="com.wec.lwkm"
	versionCode="104060"
	version   = "1.4.6">
  <name>LWKM</name>
  <description>&#xD;
    Torture your ribs with laughter&#xD;
  </description>
  <author href="wec.com.ng" email="app@wec.com.ng">Web Extreme Concept
	</author>
  <content src="index.html"/>
  <access origin="*"/>
  <access origin="*.pushwoosh.com"/>
  <access origin="*.facebook.com"/>
  <access origin="*.youtube.com"/>
   <access origin="*.wordpress.com"/>
  <access origin="*.blogspot.com"/>
  <access origin="*.ytimg.com"/>
  <access origin="*.instagram.com"/>
  <access origin="*.soundcloud.com"/>
  <allow-navigation href="*"/>
  <allow-intent href="*"/>

  <preference name="phonegap-version" value="cli-6.0.0" />
  <preference name="android-build-tool" value="gradle" />
  <preference name="android-minSdkVersion" value="19" />
	<preference name="android-targetSdkVersion" value="19" />
  <preference name="orientation" value="portrait"/>
  <preference name="fullscreen" value="false"/>
  <preference name="target-device" value="universal "/>
  <preference name="prerendered-icon" value="true"/>
   <preference name="LoadUrlTimeoutValue" value="70000"/>
   <preference name="SplashScreen" value="splash"/>
   <preference name="SplashScreenDelay" value="10000" />
   <preference name="AutoHideSplashScreen" value="false" />
	 <preference name="FadeSplashScreen" value="true" />
<preference name="FadeSplashScreenDuration" value="1.0" />
  <preference name="detect-data-types" value="true"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="stay-in-webview" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="BackupWebStorage" value="none"/>
  <preference name="Orientation" value="default"/>
  <preference name="BackgroundColor" value="0xFFFFFFCC"/>
  <preference name="KeepRunning" value="true"/>
  <!-- App Plugins -->
   <dependency id="cordova-plugin-googleplayservices" />
	<plugin name="cordova-plugin-apprate" spec="1.1.11" source="npm"/>
	<plugin name="cordova-plugin-dialogs" spec="1.2.0" source="npm"/>
	<plugin name="cordova-plugin-whitelist" spec="1.2.1" source="npm"/>
	<plugin name="cordova-plugin-admobpro" source="npm"/>
		<plugin name="cordova-facebook-sdk" spec="4.7.2" source="npm"/>
	<plugin name="cordova-plugin-inappbrowser" spec="1.3.0" source="npm"/>
		<plugin name="cordova-plugin-network-information" spec="1.2.0" source="npm"/>
	<plugin name="cordova-plugin-splashscreen" spec="2.0.0" source="npm"/>
	<plugin name="cordova-plugin-x-socialsharing" spec="5.0.11" source="npm"/>
	<plugin name="cordova-plugin-x-toast" spec="2.5.0" source="npm"/>
	<!-- <plugin name="pushwoosh-cordova-plugin" spec="4.2.2" source="npm"/> -->
  <gap:plugin name="pushwoosh-pgb-plugin" source="npm"/>
	<plugin name="cordova-plugin-google-analytics" spec="0.8.1" source="npm"/>
	<plugin name="cordova-plugin-ionic-keyboard" spec="1.0.5" source="npm"/>
	<plugin name="cordova-plugin-appsee" spec="2.2.1" source="npm"/>
	<plugin name="de.appplant.cordova.plugin.email-composer" spec="0.8.2" source="pgb"/>
    <icon src="resources\android\icon\drawable-mdpi-icon.png" gap:platform="android" gap:qualifier="mdpi"/>
    <icon src="resources\android\icon\drawable-hdpi-icon.png" gap:platform="android" gap:qualifier="hdpi"/>
    <icon src="resources\android\icon\drawable-xhdpi-icon.png" gap:platform="android" gap:qualifier="xhdpi"/>
    <icon src="resources\android\icon\drawable-xxhdpi-icon.png" gap:platform="android" gap:qualifier="xxhdpi"/>
    <icon src="resources\android\icon\drawable-xxxhdpi-icon.png" gap:platform="android" gap:qualifier="xxxhdpi"/>
    <icon src="icon.png"/>
    <splash src="resources\android\splash\drawable-land-xhdpi-screen.9.png" gap:platform="android" gap:qualifier="land-xhdpi"/>
	<splash src="splash.png" />

</widget>
